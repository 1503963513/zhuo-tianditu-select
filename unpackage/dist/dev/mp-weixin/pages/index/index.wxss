
.box {
        height: 200px;
        width: 100%;
        overflow-y: auto;
}
.box-item {
        width: 100%;
        background-color: blue;
        height: 100px;
}
.box-item:nth-child(2n) {
        background-color: red;
}
.content {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
}
.logo {
        height: 200rpx;
        width: 200rpx;
        margin-top: 200rpx;
        margin-left: auto;
        margin-right: auto;
        margin-bottom: 50rpx;
}
.text-area {
        display: flex;
        justify-content: center;
}
.title {
        font-size: 36rpx;
        color: #8f8f94;
}
